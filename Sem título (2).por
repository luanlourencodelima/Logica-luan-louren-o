programa {
  funcao inicio() {
  real valor_conta, porcentagem_gorjeta, gorjeta, total_pagar  
 escreva ("conta\n) }
escreva("valor: ")}
leia(valor_conta)

escreva("gorjeta (%): ")
leia(porcentagem_gorjeta)

gorjeta = (porcentagem_gorjeta / 100) * valor_conta
total_pagar = valor_conta + gorjeta

escreva("\nresumo:\n")
escreva("valor da conta: $ ", gorjeta, "\n")
ecreva("total a pagar: $ ", total_pagar, "\n")